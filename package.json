{
  "name": "playbackart-server",
  "description": "",
  "version": "1.4.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "krislintigo"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 18.14.2"
  },
  "feathers": {
    "language": "ts",
    "packager": "pnpm",
    "database": "mongodb",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "main": "lib/client",
  "scripts": {
    "dev": "nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf lib/ && tsc --skipLibCheck",
    "start": "node lib/",
    "prettier": "npx prettier \"**/*.ts\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "test": "npm run mocha",
    "bundle:client": "npm run compile && npm pack --pack-destination ./public"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.499.0",
    "@casl/ability": "^6.5.0",
    "@feathersjs/adapter-commons": "^5.0.21",
    "@feathersjs/authentication-client": "^5.0.21",
    "@feathersjs/authentication-local": "^5.0.21",
    "@feathersjs/configuration": "^5.0.21",
    "@feathersjs/errors": "^5.0.21",
    "@feathersjs/feathers": "^5.0.21",
    "@feathersjs/koa": "^5.0.21",
    "@feathersjs/mongodb": "^5.0.21",
    "@feathersjs/schema": "^5.0.21",
    "@feathersjs/socketio": "^5.0.21",
    "@feathersjs/transport-commons": "^5.0.21",
    "@feathersjs/typebox": "^5.0.21",
    "dayjs": "^1.11.10",
    "feathers-casl": "^2.1.1",
    "lodash": "^4.17.21",
    "mongodb": "6.3.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@feathersjs/cli": "^5.0.21",
    "@feathersjs/authentication": "^5.0.21",
    "@feathersjs/rest-client": "^5.0.21",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "10.0.6",
    "@types/node": "20.11.7",
    "@types/uuid": "^9.0.8",
    "@types/yargs": "^17.0.32",
    "axios": "1.6.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "43.0.0",
    "eslint-plugin-prettier": "5.1.2",
    "fs-extra": "^11.2.0",
    "mocha": "10.2.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "dotenv": "^16.4.1",
    "shx": "0.3.4",
    "migrate-mongo": "^11.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "yargs": "^17.7.2"
  }
}
