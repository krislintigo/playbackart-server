{
  "name": "playbackart-server",
  "description": "",
  "version": "1.2.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "1sses"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 18.12.1"
  },
  "feathers": {
    "language": "ts",
    "packager": "yarn",
    "database": "mongodb",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "main": "lib/client",
  "scripts": {
    "dev": "nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf lib/ && tsc --skipLibCheck",
    "start": "node lib/",
    "prettier": "npx prettier \"**/*.ts\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "test": "npm run mocha",
    "bundle:client": "npm run compile && npm pack --pack-destination ./public"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.379.1",
    "@casl/ability": "^6.5.0",
    "@feathersjs/adapter-commons": "5.0.8",
    "@feathersjs/authentication": "5.0.8",
    "@feathersjs/authentication-client": "5.0.8",
    "@feathersjs/authentication-local": "5.0.8",
    "@feathersjs/authentication-oauth": "5.0.8",
    "@feathersjs/configuration": "5.0.8",
    "@feathersjs/errors": "5.0.8",
    "@feathersjs/feathers": "5.0.8",
    "@feathersjs/koa": "5.0.8",
    "@feathersjs/mongodb": "5.0.8",
    "@feathersjs/schema": "5.0.8",
    "@feathersjs/socketio": "5.0.8",
    "@feathersjs/transport-commons": "5.0.8",
    "@feathersjs/typebox": "5.0.8",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "feathers-casl": "^2.1.0",
    "lodash": "^4.17.21",
    "migrate-mongo": "^10.0.0",
    "mongodb": "5.7.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@feathersjs/cli": "5.0.8",
    "@feathersjs/rest-client": "5.0.8",
    "@types/lodash": "^4.14.195",
    "@types/mocha": "10.0.1",
    "@types/node": "20.4.4",
    "@types/uuid": "^9.0.2",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "axios": "1.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "10.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "yargs": "^17.7.2"
  }
}
